{
  "name": "domain-zero-e2e-tests",
  "version": "1.0.0",
  "description": "Playwright E2E tests for Domain Zero Protocol",
  "private": true,
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "report": "playwright show-report",
    "install:browsers": "playwright install --with-deps"
  },
  "keywords": [
    "playwright",
    "e2e",
    "testing",
    "domain-zero"
  ],
  "author": "Domain Zero Protocol",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0"
  }
}
